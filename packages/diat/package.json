{
  "name": "diat",
  "version": "1.5.0",
  "description": "A CLI tool to help with diagnosing Node.js processes basing on inspector.",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build && ./bin/diat.js --help",
    "build": "tsc",
    "prepublish": "npm run build && cp ../../README_EN.md ./README.md",
    "test": "jest -c jest.config.json --coverage --coveragePathIgnorePatterns=__tests__ --passWithNoTests -w=1 --forceExit",
    "test:ci": "node scripts/ci",
    "prettier": "prettier src/**/*.ts --write"
  },
  "bin": {
    "diat": "./bin/diat.js"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "snippet",
    "bin",
    "lib",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "ansi-escapes": "^4.3.0",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "diat-linux-perf": "^1.3.0",
    "diat-live-inspector": "^1.3.0",
    "diat-node-inspect": "^1.3.0",
    "diat-stackvis-simplified": "^1.3.0",
    "inquirer": "^7.0.0",
    "node-fetch": "^2.6.0",
    "semver": "^6.3.0",
    "serve-static": "^1.14.1",
    "tslib": "^1.10.0",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.7",
    "@types/node": "^13.9.0",
    "jest": "^24.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^3.8.3"
  }
}
